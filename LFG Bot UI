# UI code
import tkinter as tk

# Create a window
window = tk.Tk()

# Create a label for the post ID
label_post_id = tk.Label(text="Post ID")
label_post_id.grid(row=0, column=0)

# Create an entry for the post ID
entry_post_id = tk.Entry()
entry_post_id.grid(row=0, column=1)

# Create a label for the LFG posting
label_lfg_posting = tk.Label(text="LFG Posting")
label_lfg_posting.grid(row=1, column=0)

# Create an entry for the LFG posting
entry_lfg_posting = tk.Entry()
entry_lfg_posting.grid(row=1, column=1)

# Create a label for the players needed
label_players_needed = tk.Label(text="Players Needed")
label_players_needed.grid(row=2, column=0)

# Create an entry for the players needed
entry_players_needed = tk.Entry()
entry_players_needed.grid(row=2, column=1)

# Create a label for the time and date
label_time_and_date = tk.Label(text="Time and Date")
label_time_and_date.grid(row=3, column=0)

# Create an entry for the time and date
entry_time_and_date = tk.Entry()
entry_time_and_date.grid(row=3, column=1)

